#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "libft.h"

int     main(int argc, char **argv)
{
    char *str1;
    char *str2;

    (void)argc;
    (void)argv;
    str1 = strdup("Hello World!");
    str2 = strdup("Hello World!");
    printf("%s\n%s\n", str1, str2);
    memset(str1, 'B', 4);
    ft_memset(str2, 'B', 4);
    printf("%s\n%s\n", str1, str2);
}